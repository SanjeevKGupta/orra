{
    "org": "${HZN_ORG_ID}",
    "label": "${SERVICE_NAME}-${ARCH}",
    "url": "${SERVICE_NAME}",
    "version": "${SERVICE_VERSION}",
    "arch": "${ARCH}",
    "public": false,
    "sharable": "singleton",
    "requiredServices": [
    ],
    "userInput": [
    ],
    "deployment": {
        "services": {
            "${SERVICE_NAME}": {
                "image": "${DOCKER_IMAGE_BASE}:${DOCKER_IMAGE_VERSION}",
                "command": ["agent", "-ui", "-bootstrap", "-server", "-client", "0.0.0.0"],
                "binds": [
                    "${APP_BIND_HORIZON_DIR}/consul-config:/consul/config:rw",
                    "${APP_BIND_HORIZON_DIR}/consul-data:/consul/data:rw"
                ],
                "ports": [
                    {
                        "HostIP": "0.0.0.0",
                        "HostPort": "8500:8500/tcp"
                    }
                ]
            }
        }
    }
}
