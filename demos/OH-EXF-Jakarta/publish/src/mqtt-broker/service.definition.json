{
    "org": "${HZN_ORG_ID}",
    "label": "${SERVICE_NAME}-${ARCH}",
    "url": "${SERVICE_NAME}",
    "version": "${SERVICE_VERSION}",
    "arch": "${ARCH}",
    "public": false,
    "sharable": "singleton",
    "requiredServices": [
    ],
    "userInput": [
    ],
    "deployment": {
        "services": {
            "${SERVICE_NAME}": {
                "image": "${DOCKER_IMAGE_BASE}:${SERVICE_VERSION}",
                "command": ["/usr/sbin/mosquitto", "-c", "/mosquitto-no-auth.conf"],
                "ports": [{
                    "HostIP": "0.0.0.0",
                    "HostPort": "1883:1883/tcp"
                }]
            }
        }
    }
}
